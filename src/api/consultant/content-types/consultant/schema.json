{
  "kind": "collectionType",
  "collectionName": "consultants",
  "info": {
    "singularName": "consultant",
    "pluralName": "consultants",
    "displayName": "Consultant",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "reg_Num": {
      "type": "string",
      "required": true,
      "minLength": 6,
      "unique": true
    },
    "Email": {
      "type": "email",
      "required": true
    },
    "Address": {
      "type": "text",
      "required": true
    },
    "Name": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "LicenseNumber": {
      "type": "string",
      "required": true
    },
    "hospitals": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::hospital.hospital",
      "mappedBy": "consultants"
    },
    "medical_records": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::medical-record.medical-record",
      "mappedBy": "consultant"
    },
    "Password": {
      "type": "string",
      "required": true
    },
    "LicenseImg": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "doctor_Pic": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "appointments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::appointment.appointment",
      "mappedBy": "consultant"
    },
    "Type_of_Spec": {
      "type": "string"
    },
    "NationalID": {
      "type": "string"
    }
  }
}
