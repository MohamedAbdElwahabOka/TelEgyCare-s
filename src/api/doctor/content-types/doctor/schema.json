{
  "kind": "collectionType",
  "collectionName": "doctors",
  "info": {
    "singularName": "doctor",
    "pluralName": "doctors",
    "displayName": "Doctor",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Email": {
      "type": "email",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "Address": {
      "type": "text",
      "required": true
    },
    "Type_of_Spec": {
      "type": "enumeration",
      "enum": [
        "Anatomical Pathology",
        "Anesthesiology",
        "Cardiology",
        "Cardiovascular/Thoracic Surgery",
        "Clinical Immunology/Allergy",
        "Critical Care Medicine",
        "Dermatology"
      ],
      "required": true
    },
    "doctor_Pic": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "reg_Num": {
      "type": "string",
      "unique": true,
      "minLength": 6,
      "required": true,
      "regex": "^\\d{6,50}$"
    },
    "hospital": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::hospital.hospital",
      "inversedBy": "doctors"
    },
    "medical_records": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::medical-record.medical-record",
      "mappedBy": "doctor"
    },
    "appointments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::appointment.appointment",
      "mappedBy": "doctor"
    },
    "Password": {
      "type": "string",
      "required": true
    },
    "LicenseNumber": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "LicenseImg": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "NationalID": {
      "type": "string",
      "required": true,
      "unique": true
    }
  }
}
