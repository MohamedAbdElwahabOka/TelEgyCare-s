{
  "kind": "collectionType",
  "collectionName": "receptionists",
  "info": {
    "singularName": "receptionist",
    "pluralName": "receptionists",
    "displayName": "Receptionist",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "reg_Num": {
      "type": "string",
      "unique": true,
      "required": true,
      "minLength": 6,
      "regex": "^\\d{6,50}$"
    },
    "Name": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "Address": {
      "type": "text",
      "required": true
    },
    "hospital": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::hospital.hospital",
      "inversedBy": "receptionists"
    },
    "appointments": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::appointment.appointment",
      "mappedBy": "receptionists"
    },
    "Password": {
      "type": "text",
      "required": true,
      "unique": false
    }
  }
}
